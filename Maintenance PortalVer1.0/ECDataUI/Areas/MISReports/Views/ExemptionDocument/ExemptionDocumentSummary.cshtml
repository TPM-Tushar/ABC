@model CustomModels.Models.MISReports.ExemptionDocument.ExemptionDocumentSummary
    @using ECDataUI.Helper

    <style>
        /*td:nth-last-child(1), td:nth-last-child(2), td:nth-last-child(3) {
            text-align: right;
        }

        td:nth-last-child(5){
            text-align: left;
        }

        td:nth-last-child(4), td:nth-last-child(6) {
            text-align: center;
        }*/

    </style>

    <div class="container">


        <table id="DROfficeWiseSummaryTableID" class="table table-striped table-bordered table-condensed table-hover" style="padding-top:1% ;vertical-align: middle;background-color: #99CFF7; overflow: hidden; border: solid; width: 80%;margin-left:10%;">
            <thead>
                <tr>
                    <th>Serial No</th>
                    <th>Jurisdictional Office<br /></th>
                    <th>SRO Name<br /></th>
                    <th>Documents<br /></th>
                    <th>Total Stamp Before Excemption</th>
                    <th>Total Excemption Given</th>
                    <th>Total Stamp After Excemption</th>
                    <th>Total Registration Fees<br /></th>
                    <th>Total<br /></th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td style="text-align:center">@Model.SerialNo</td>
                    <td style="text-align:left">@Model.JurisdictionalOffice</td>
                    <td style="text-align:left">@Model.SROName</td>
                    <td style="text-align:center">@Model.Documents</td>
                    <td style="text-align:right">@Model.STAMPDUTY_BEFORE_EXEMPTION.ToString("N2", System.Globalization.CultureInfo.CreateSpecificCulture("hi-IN"))</td>
                    <td style="text-align:right">@Model.EXEMPTION_GIVEN.ToString("N2", System.Globalization.CultureInfo.CreateSpecificCulture("hi-IN"))</td>
                    <td style="text-align:right">@Model.STAMPDUTY_AFTER_EXEMPTION.ToString("N2", System.Globalization.CultureInfo.CreateSpecificCulture("hi-IN"))</td>
                    <td style="text-align:right">@Model.RegistrationFees.ToString("N2", System.Globalization.CultureInfo.CreateSpecificCulture("hi-IN"))</td>
                    <td style="text-align:right">@Model.Total.ToString("N2", System.Globalization.CultureInfo.CreateSpecificCulture("hi-IN"))</td>
                </tr>
            </tbody>
        </table>
    </div>