<script src="~/Areas/MISReports/Scripts/DataTransmissionDetails/DataTransmissionDetails.js"></script>

<div class="container">

    <div class="col-md-offset-0">

        <h3 style="text-align:center;color:#3177b4 ;">Data Transmission Report<span style="color:black"></span></h3>
        <div class="panel panel-default" id="panelNewBranch" style="width: 100%; box-shadow: 5px 5px 10px #d0d3d4; background: #FBFCFC; display: block;">

            <div class="panel-body">
                <div class='row well-custom' style="height: 35px; padding-top: 10px;">
                    <span id="" style="margin-left: 05px; font-size: 120%; padding-left: 1%;"></span>
                    <span class="" style="color: #FFF; font-size: 120%;">
                        Search Parameters
                    </span>
                </div>
                <div class="" style="text-align: center; background: #fff; border: 1px solid #ECF0F1; padding-top: 1%;padding-bottom:1%;">
                    <form id="SearchParametersForm">
                        @Html.AntiForgeryToken()
                        @Html.ValidationSummary(true)

                        <div class="row" style="padding-top:1%;padding-bottom:1%;" id="HiddedRow">
                            <div class="col-md-12">
                                <div class="col-md-2">   </div>
                                <div class="col-md-4">
                                    <div class="col-md-4">
                                        @Html.LabelFor(model => model.DataBaseList, new { @class = "PaddingTop10" }, Mandatory.Yes)
                                    </div>
                                    <div class="col-md-8">
                                        @Html.DropDownListFor(model => model.DBName, @Model.DataBaseList, new { @class = "form-control minimal", id = "DataBaseListID", autocomplete = "off", tabindex = 1 })
                                        @Html.ValidationMessageFor(model => model.DBName, "", new { @class = "text-danger" })
                                    </div>
                                </div>

                                <div class="col-md-4">
                                    <div class="col-md-4">
                                        @Html.LabelFor(model => model.SROList, new { @class = "PaddingTop10" }, Mandatory.Yes)
                                    </div>
                                    <div class="col-md-8">
                                        @Html.DropDownListFor(model => model.SROID, @Model.SROList, new { @class = "form-control minimal", id = "SROOfficeListID", autocomplete = "off", tabindex = 1 })
                                        @Html.ValidationMessageFor(model => model.SROID, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="col-md-2">   </div>
                            </div>
                        </div>
                    </form>
                </div>


            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class='BTNwrapper' style="margin-top:1%;margin-bottom:2%;">
                        <button type="button" class="btn btn-group-md btn-success" id="SearchBtn" tabindex="6">Search</button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="panel panel-default" id="SearchResult" style="width: 100%; box-shadow: 5px 5px 10px #d0d3d4; background: #FBFCFC; display: block;">
        <div class="panel-body">
            <div id='DtlsSearchParaListCollapse' class="row well-custom collapsed" data-toggle="collapse" data-target="#DtlscollapseSearchPara" aria-expanded="false" aria-controls="DtlscollapseSearchPara" style="height: 35px; padding-top: 10px; cursor: pointer;">
                <span style="cursor: pointer; margin-bottom: 20px;">
                    <span id="DtlsToggleIconSearchPara" class="fa fa-plus-square-o fa-pull-left fa-2x" style="margin-left: 05px; font-size: 103%; padding-top: 2px; color: #FFF"></span>
                </span>
                <span class="" style="color: #fff; font-size: 120%;">Data Transmission Report</span>
            </div>
            <div id="DtlscollapseSearchPara" class="collapse" style="text-align: center; background: #fff; border: 1px solid #ECF0F1 !important; padding-top: 1%; border-radius: 5px;">

                <div class="clsTblWrapper" style="text-align: center; margin-left: 1%; margin-right: 1%;">
                    <div class="row" style="text-align:right;"><i><h6><span class="Blink" style="text-align:right"> </span><b><span style="color:red;"></span>@*Note : This report is based on pre processed data considered upto*@ @*@Model.MaxDate*@</b> </h6></i></div>

                    <table id="DailyReceiptsID" class="table table-striped table-bordered table-condensed table-hover" style="vertical-align: middle;background-color: aliceblue; overflow: hidden; border: solid; width: 100%;">
                        <thead>
                            <tr>
                                <th style="vertical-align:middle;">S. No.</th>
                                <th style="vertical-align:middle;">Table Name</th>
                                <th style="vertical-align:middle;">Row Count</th>
                            </tr>
                        </thead>
                    </table>
                </div>

                <div class="row" style="padding-bottom:1%;">
                    <div class="col-md-5"></div>

                    <div class='BTNwrapper col-md-2' style="text-align:left;">
                        <span id="EXCELSPANID"></span>

                    </div>
                    <div class="col-md-5"></div>

                </div>
            </div>
        </div>
    </div>
</div>

