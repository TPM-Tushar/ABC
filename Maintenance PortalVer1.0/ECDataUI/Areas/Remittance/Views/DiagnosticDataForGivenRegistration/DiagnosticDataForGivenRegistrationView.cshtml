<div class="container">
    @*<h3 style="text-align:center;">Diagnostic Data For Given Registration</h3>*@
    <h3 style="text-align:center;">Record Analysis For Given Registration</h3>
    <div class='row'>
        <div class="panel panel-default" id="panelNewBranch" style="width: 100%; box-shadow: 5px 5px 10px #d0d3d4; background: #FBFCFC; display: block;">
            <div class="panel-body">
                <div class='row well-custom' style="height: 35px; padding-top: 10px;">
                    <span id="" style="margin-left: 05px; font-size: 120%; padding-left: 1%;"></span>
                    <span class="" style="color: #FFF; font-size: 120%;">
                        Search Parameters
                    </span>
                </div>
                <form id="DiagnosticDataForGivenRegistrationFormID">

                    @Html.AntiForgeryToken()
                    @Html.ValidationSummary(true)
                    <div class="row" style="text-align: center; background: #fff; border: 1px solid #ECF0F1; padding-top: 1%;padding-bottom:2%;">
                        <div class="col-md-12">

                            <div class="row">
                                <div class="col-md-2"></div>
                                <div class="col-md-4">
                                    <div class="col-md-5">
                                        @Html.LabelFor(x => x.RegistrationModuleCode, new { @class = "PaddingTop10" }, Mandatory.Yes)
                                    </div>
                                    <div class="col-md-7">
                                        @Html.DropDownListFor(x => x.RegistrationModuleCode, Model.RegistrationModuleList, new { @class = "form-control minimal", @Id = "RegistrationModuleDropDownID", @tabindex = 1 })
                                        @Html.ValidationMessageFor(x => x.RegistrationModuleCode, "", new { @class = "text-danger" })
                                    </div>
                                </div>

                                <div class="col-md-4">
                                    <div class="col-md-5">
                                        @Html.LabelFor(model => model.FinalRegistrationNumber, new { @class = "PaddingTop10", style = "padding-left:4px;" }, Mandatory.Yes)
                                    </div>
                                    <div class="col-md-7">
                                        <div class="date" id="divFinalRegistrationNo" style="display: table; width: 102%; cursor: pointer;">
                                            @Html.TextBoxFor(model => model.FinalRegistrationNumber, new { @class = "form-control AllowOnlyNumber", @id = "txtFinalRegNo", @placeholder = "", @title = "Rows", tabindex = 4 })
                                            @Html.ValidationMessageFor(model => model.FinalRegistrationNumber, "", new { @class = "text-danger" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-2"></div>
                            </div>


                            <div class="row">
                            </div>

                            <div class="row" style="margin-top:1%;">
                                <div class="col-md-4"> </div>
                                <div class="col-md-4">
                                    @*<button type="button" class="btn btn-group-md btn-primary" id="SearchBtn">Search</button>&nbsp;&nbsp;&nbsp;&nbsp;*@
                                    <button type="button" class="btn btn-group-md btn-success" id="DownloadBtn" tabindex="6">Download</button>
                                    @*<button type="button" class="btn btn-group-md btn-warning" id="ResetID">Reset</button>*@
                                </div>
                                <div class="col-md-4"> </div>
                            </div>
                        </div>
