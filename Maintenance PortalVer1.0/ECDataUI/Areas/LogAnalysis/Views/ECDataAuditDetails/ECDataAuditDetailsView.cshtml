@model CustomModels.Models.LogAnalysis.ECDataAuditDetails.ECDataAuditDetailsRequestModel
@using ECDataUI.Helper

<style>
     .td {
        font-size: 50px;
    }
</style>

<link href="~/Areas/LogAnalysis/CSS/ECAuditDataTableStyles.css" rel="stylesheet" />
@*<script src="~/Areas/LogAnalysis/Scripts/ECDataAuditDetails/ECDataAuditDetails.js"></script>*@
<script src="~/Areas/LogAnalysis/Scripts/ECDataAuditDetails/ECDataAuditDetails.min.js"></script>
<div class="container">
    <div class="col-md-offset-0">


        <div class="panel panel-default" id="panelNewBranch" style="width: 100%; box-shadow: 5px 5px 10px #d0d3d4; background: #FBFCFC; display: block;">
            <div class="panel-body">
                <div class='row well-custom' style="height: 35px; padding-top: 10px;">
                    <span id="" style="margin-left: 05px; font-size: 120%; padding-left: 1%;"></span>
                    <span class="" style="color: #FFF; font-size: 120%;">
                        Search Parameters
                    </span>
                </div>
                <div class="" style="text-align: center; background: #fff; border: 1px solid #ECF0F1; padding-top: 1%;">
                    <form id="SearchParametersForm">


                        @Html.AntiForgeryToken()
                        @Html.ValidationSummary(true)

                        <div class='row'>
                            <div class="col-md-12 textAlignLeft">


                                <div class="col-md-3">

                                    <div class="col-md-5">
                                        @Html.LabelFor(model => model.FromDate, new { @class = "PaddingTop10", style = "padding-left:4px;" }, Mandatory.Yes)
                                    </div>
                                    <div class="col-md-7">
                                        <div class="date" id="divFromDate" style="display: table; width: 102%; cursor: pointer;">
                                            @Html.TextBoxFor(model => model.FromDate, new { @class = "form-control", @id = "txtFromDate", @tabindex = 5, @placeholder = "dd/mm/yyyy", @title = "From Date", @maxlength = 10 })
                                            <span class="input-group-addon" style="margin-left: 2px; border: 1px solid #ccc; border-radius: 5px;"><span class="glyphicon glyphicon-calendar CalendarIcon" style=""></span></span>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-3">

                                    <div class="col-md-5">
                                        @Html.LabelFor(model => model.ToDate, new { @class = "PaddingTop10", style = "padding-left:4px;" }, Mandatory.Yes)
                                    </div>
                                    <div class="col-md-7">
                                        <div class="date" id="divToDate" style="display: table; width: 102%; cursor: pointer;">
                                            @Html.TextBoxFor(model => model.ToDate, new { @class = "form-control", @id = "txtToDate", @tabindex = 5, @placeholder = "dd/mm/yyyy", @title = "To Date", @maxlength = 10 })
                                            <span class="input-group-addon" style="margin-left: 2px; border: 1px solid #ccc; border-radius: 5px;"><span class="glyphicon glyphicon-calendar CalendarIcon" style=""></span></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="col-md-4">
                                        @Html.LabelFor(model => model.OfficeList, new { @class = "PaddingTop10" }, Mandatory.Yes)

                                    </div>
                                    <div class="col-md-8">

                                        @Html.DropDownListFor(model => model.OfficeID, @Model.OfficeList, new { @class = "form-control minimal", id = "ddOfficeList", autocomplete = "off" })
                                        @Html.ValidationMessageFor(model => model.OfficeID, "", new { @class = "text-danger" })

                                    </div>
                                </div>

                                <div class="col-md-3">
                                    <div class="col-md-6">
                                        @Html.LabelFor(model => model.ProgramNameList, new { @class = "PaddingTop10" }, Mandatory.Yes)

                                    </div>
                                    <div class="col-md-6">
                                        @Html.ListBoxFor(model => model.ProgramID, @Model.ProgramNameList, new { @class = "form-control", @id = "ddProgramList", autocomplete = "off" })
                                        @Html.ValidationMessageFor(model => model.ProgramID, "", new { @class = "text-danger" })

                                    </div>
                                </div>
                            </div>
                        </div>
                        <br />
                        <br />
                        <br />
                        <div class="row">
                            <div class="col-md-12">
                                <div class="col-md-offset-2">
                                    @* <div class="form-group col-md-6">
                        <div class="input-container" id="dvCaptcha">
                            <div class='col-md-6'>
                                @Html.LabelFor(model => model.Captcha, new { @class = "PaddingTop10" }, Mandatory.Yes)

                            </div>
                            <div class='col-md-4'>
                                @Html.CaptchaFor(model => model.Captcha, "GetCaptchaImage", "ECDataAuditDetails", null, "Try Another Image? Click Here", new { @title = "Enter the text you see in an image", @class = "captcha-registration", @id = "txtCaptcha", @style = "text-align:center;", autocomplete = "off" })
                                @Html.ValidationMessageFor(model => model.Captcha, "", new { @class = "text-danger" })
                            </div>


                        </div>
                    </div>*@
                                    <div class="form-group col-md-8">
                                        <div class='BTNwrapper'>
                                            <button type="button" class="btn btn-group-md btn-success" id="btnSearch">Search</button>
                                        </div>
                                        <style>
                                            .BTNwrapper {
                                                text-align: center;
                                            }

                                            .button {
                                                position: absolute;
                                                top: 50%;
                                            }
                                        </style>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>

            </div>
        </div>
        <div class="panel panel-default" id="panelNewBranch" style="width: 100%; box-shadow: 5px 5px 10px #d0d3d4; background: #FBFCFC; display: block;">

            <div class="panel-body">
                <div id='SearchParaListCollapse' class="row well-custom collapsed" data-toggle="collapse" data-target="#collapseSearchPara" aria-expanded="false" aria-controls="collapseSearchPara" style="height: 35px; padding-top: 10px; cursor: pointer;">

                    <span style="cursor: pointer; margin-bottom: 20px;">
                        <span id="ToggleIconSearchPara" class="fa fa-plus-square-o fa-pull-left fa-2x" style="margin-left: 05px; font-size: 103%; padding-top: 2px; color: #FFF"></span>
                    </span>
                    <span class="" style="color: #fff; font-size: 120%;">Officewise Summary</span>
                </div>
                <div id="collapseSearchPara" class="collapse" style="text-align: center; background: #fff; border: 1px solid #ECF0F1 !important; padding-top: 1%; overflow-y: auto; max-height: 400px; border-radius: 5px;">
                    <div class="col-md-12">
                        <div class="panel panel-default">
                            <div class="panel-body">
                                <div class="row">
                                    <div id="OfficeWiseTblWrapper" class="col-md-12" style="MAX-HEIGHT: 220px; overflow: auto !important;">
                                        <div class="col-md-3">
                                            <div class="col-md-3" style="margin-top: 3px !important;">
                                            </div>
                                            <div class="col-md-8" style="margin-top: 3px !important;">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <table id="OfficeWiseTbl" class="table table-striped table-bordered table-condensed table" style="min-width: 250px !important;">
                                                <thead>
                                                    <tr>
                                                        <th style="text-align: center;">Offices <i>(<span id="TotModfctnOfcCount"></span>)</i></th>
                                                        <th style="text-align: center;">Total No. Of Modifications  <i>(<span id="TotModfctnCount"></span>)</i></th>
                                                        <th style="text-align: center;">Last Modification Date</th>
                                                        @* <th style="text-align: center;">Status</th>*@
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr>
                                                        <td style='cursor: pointer;' class='text-success' onclick="LoadOfficeWise()"></td>
                                                        <td></td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                        <div class="col-md-3">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <br>
            </div>
        </div>
        <br />

        <div class="panel panel-default" id="SearchResult" style="width: 100%; box-shadow: 5px 5px 10px #d0d3d4; background: #FBFCFC; display: block;">
            <div class="panel-body">
                <div id='DtlsSearchParaListCollapse' class="row well-custom collapsed" data-toggle="collapse" data-target="#DtlscollapseSearchPara" aria-expanded="false" aria-controls="DtlscollapseSearchPara" style="height: 35px; padding-top: 10px; cursor: pointer;">

                    <span style="cursor: pointer; margin-bottom: 20px;">
                        <span id="DtlsToggleIconSearchPara" class="fa fa-plus-square-o fa-pull-left fa-2x" style="margin-left: 05px; font-size: 103%; padding-top: 2px; color: #FFF"></span>
                    </span>
                    <span class="" style="color: #fff; font-size: 120%;">Details</span>
                </div>

                @* <div id="DtlscollapseSearchPara" class="collapse" style="text-align: center; background: #fff; border: 1px solid #ECF0F1; padding-top: 1%;">*@
                <div id="DtlscollapseSearchPara" class="collapse" style="text-align: center; background: #fff; border: 1px solid #ECF0F1 !important; padding-top: 1%; overflow-y: auto; max-height: 400px; border-radius: 5px;">

                    <div class="clsTblWrapper" style="text-align: center; margin-left: 1%; margin-right: 1%; margin-bottom: 0%;">
                        <table id="StatTableDataID" class="table table-striped table-bordered table-condensed table-hover" style="vertical-align: middle;background-color: aliceblue; overflow: hidden; border: solid; width: 100%;">
                            <thead>
                                <tr>
                                    <th>SRO Name</th>
                                    <th>Final Registration Number</th>
                                    <th>Date Of Modification</th>
                                    <th>Modification Area</th>
                                    <th>Modification Type</th>
                                    <th>Modification Details</th>
                                    <th>Application/Tools</th>
                                    <th>IP Address</th>
                                    <th>Host Name</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                    <div class="row" style="margin-bottom: 2%;">
                        <div class="col-md-5"></div>
                        <div class="col-md-2">
                            <div class='BTNwrapper col-md-12' style="padding-left:5%;">
                                @*<button type="button" style="width:75%;" class="btn btn-group-md btn-warning" id="PdfDownload"><i style="padding-right:3%;" class="fa fa-file-pdf-o"></i>Download as PDF</button>*@
                                <span id="PDFSPANID"></span>
                            </div>

                        </div>
                        <div class="col-md-5" style="padding-left:0%;">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="MasterTableModel" style="margin-left: 31%;" class="modal fade " role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
</div>