                            </div>


                            <!-- FOCUS HEADING -->
                            <p>
                                <div class="card" style="margin-left:4%; padding-top:0% !important;">
                                    <form method="POST" id="LoginformId">


                                        @Html.AntiForgeryToken()
                                        @Html.ValidationSummary(true)


                                        <div class="input-container">
                                            @*<input type="text" id="#{label}" required="required" type="#{type}">
                                                <label for="#{label}">Username</label>*@

                                            @Html.TextBoxFor(model => model.EmailId, new { @class = "", @id = "txtEmail", @placeholder = "Username", @tabindex = 1 })
                                            @Html.ValidationMessageFor(model => model.EmailId, "", new { @class = "text-danger" })
                                            <div class="bar"></div>
                                        </div>
                                        <div class="input-container">
                                            @*<input type="password" id="#{label}" required="required" type="#{type}">
                                                <label for="#{label}">Password</label>*@

                                            @Html.PasswordFor(model => model.Password, new { @class = "", @id = "txtPassword", autocomplete = "off", @placeholder = "Password", @tabindex = 2 })

                                            @Html.ValidationMessageFor(model => model.Password, "", new { @class = "text-danger" })
                                            <div class="bar"></div>
                                        </div>



                                        <div class="input-container" id="CaptchaDiv">


                                            @Html.CaptchaFor(model => model.Captcha, "GetCaptchaImage", "Login", null, "Try Another Image? Click Here", new { @title = "Enter the text you see in an image", @class = "captcha-registration", @id = "CaptchaImage", @style = "text-align:center;", @placeholder = "Enter Captcha", autocomplete = "off", @tabindex = 5 })<br>
                                            @Html.ValidationMessageFor(model => model.Captcha, "", new { @class = "text-danger" })
                                            <div class="bar"></div>
                                        </div>




                                        <div class="button-container">
                                            <button type="button" id="btnlogin" tabindex="4"><span>Sign In</span></button>&nbsp;&nbsp;&nbsp;&nbsp;
                                            <button type="reset" id="btnReset" class="reset-btn" tabindex="5"><span>Reset</span></button>
                                        </div>
                                        <div class="footer">
                                            <div>
                                                @*  <a style="color:#808080; font-size:12px;" href="@Url.Action("ForgotPassword", "Account")" title="Forgot Password">Forgot password</a> &nbsp; | &nbsp;
                                                    <a style="color:#808080; font-size:12px;" href="@Url.Action("AccountActivation", "Account")">Unlock Account</a>&nbsp; | &nbsp;
                                                    <a style="color:#808080; font-size:12px;" href="@Url.Action("UserRegistration", "Account")">Register Here</a>*@


                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12 text-center" style="color: #808080; font-size: 14px;">
                                                            Forgot your password? <i><a href="@Url.Action("ForgotPassword", "Account")" tabindex="6" style="color: #FA6541;font-weight: bold;text-decoration: underline;">Click here</a></i>
                                                        </div>
                                                    </div>
                                                </div>
                                                @*<div class="form-group login-panel-bottom" style="color: #0078d7; font-weight: bold;margin-bottom: 9px;">
                                                        <div class="row" style="padding-top: 1%; padding-bottom: 1%;">
                                                            <div class="col-md-12 text-center" style="margin-top: -8px;">
                                                                <a tabindex="7" href="@Url.Action("UserRegistration", "Account")"><i style="font-size: 17px; color: #0078d7;" class="fa fa-user-plus"></i>Register as new user</a>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div>
                                                        <div class="row">
                                                            <div class="col-md-12 text-center" style="color: #808080;">
                                                                <a tabindex="8" href="@Url.Action("AccountActivation", "Account")"><i class="fa fa-check-square"></i>&nbsp;&nbsp;Activate Account</a>
                                                            </div>
                                                        </div>
                                                    </div>*@

                                                @*<div>
                                                        <div class="row" style="padding-top:2%">
                                                            <div class="col-md-12 text-center" style="color: #808080;">
                                                                <a tabindex="9" href="@Url.Action("SearchCertificate", "SearchCertificate")"><i class="fa fa-search"></i>&nbsp;&nbsp;Search Certificate</a>
                                                            </div>
                                                        </div>
                                                    </div>*@

                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </p>
                            @*</span>*@
                        </div>
                    </div>

                    <div class="col-md-4"></div>
                </div>
                @*</span>*@

