            height: 47px !important;
            margin-bottom: 2%;
            margin-top: 2%;
        }

            .buttonEnter span {
                cursor: pointer;
                display: inline-block;
                position: relative;
                transition: 0.5s;
            }

                .buttonEnter span:after {
                    content: '\00bb';
                    position: absolute;
                    opacity: 0;
                    top: 0;
                    right: -20px;
                    transition: 0.5s;
                }

            .buttonEnter:hover span {
                padding-right: 25px;
            }

                .buttonEnter:hover span:after {
                    opacity: 1;
                    right: 0;
                }


